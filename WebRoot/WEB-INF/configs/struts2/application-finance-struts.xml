						<param name="root">smsg</param>
					</result>
					<result name="addsuccess" type="json">
						<param name="root">smsg</param>
					</result>
					<result name="updatesuccess" type="json">
						<param name="root">smsg</param>
					</result>
					<result name="deletesuccess" type="json">
						<param name="root">smsg</param>
					</result>
